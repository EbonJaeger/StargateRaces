<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <!--==== Tel'tak============-->

  <ThingDef ParentName="BuildingBase">
    <defName>Teltak</defName>
    <label>Tel'tak</label>
    <description>Tel'taks are small transport ships capable of carrying people, items, or animals. Can be used for sending gifts, transporting people or supplies, surprise attacks, or reinforcing battles.</description>
    <graphicData>
      <texPath>Ships/Teltak</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(12,12)</drawSize>
      <shadowData>
        <volume>(0.1, 0.1, 0.1)</volume>
      </shadowData>
    </graphicData>
    <size>(7,8)</size>
    <tickerType>Normal</tickerType>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.8</fillPercent>
    <designationCategory>Ship</designationCategory>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <statBases>
      <MaxHitPoints>1400</MaxHitPoints>
      <WorkToBuild>24000</WorkToBuild>
      <Flammability>0.5</Flammability>
      <Mass>114514</Mass>
    </statBases>
    <costList>
      <Plasteel>250</Plasteel>
      <Steel>1500</Steel>
      <ComponentIndustrial>16</ComponentIndustrial>
      <ComponentSpacer>5</ComponentSpacer>
      <Naquadah>100</Naquadah>
    </costList>
    <comps>
      <li Class="CompProperties_Transporter">
        <restEffectiveness>0.8</restEffectiveness>
        <massCapacity>8500</massCapacity>
      </li>
      <li Class="SRTS.CompProperties_LaunchableSRTS" />
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>3000.0</fuelCapacity>
        <fuelConsumptionRate>0.8</fuelConsumptionRate>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>3000</initialConfigurableTargetFuelLevel>
        <fuelFilter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </fuelFilter>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <autoRefuelPercent>1</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
        <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
      </li>
    </comps>
    <inspectorTabs>
      <li>ITab_TransporterContents</li>
    </inspectorTabs>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>Tel-tak</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <uiIconScale>0.65</uiIconScale>
  </ThingDef>

